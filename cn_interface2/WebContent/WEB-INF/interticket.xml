<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="
       http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd
       http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd">

	
	<bean id="InternationalAirTickets"
        class="com.ccservice.b2b2c.atom.interticket.InternationalAirTickets">
		<property name="userid" value=""/>
		<property name="username" value=""/>
		<property name="password" value=""/>
		<property name="officeNumber" value="SHA255"></property>
		<property name="ipAddress" value="http://www.lcair.com/api/price.php"/>
	</bean>
	
	<bean id="CreateTicketOrder"
        class="com.ccservice.b2b2c.atom.ticketorder.CreateTicketOrder">
	</bean>
	<bean id="ATMInternalTicket"
        class="com.ccservice.b2b2c.atom.interticket.ATMInternalTicket">
	</bean>
	<bean id="interTicker"
        class="com.ccservice.b2b2c.atom.interticket.InterTicker">
	</bean>
</beans>



